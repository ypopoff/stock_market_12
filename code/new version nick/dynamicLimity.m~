function [ ymin, ymax ] = dynamicLimity( a, d, i, deltatau )
%DYNAMICLIMITY computes the limit of the price graph
%   The limits adapt itself to the min & max price


    if i <= deltatau
        ymin = 75;
        ymax = 125;
        
    else
        if a > ymax || a < ymin || d > ymax || d < ymin
            
            
            
        end


end

